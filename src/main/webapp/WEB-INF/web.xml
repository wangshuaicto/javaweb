<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <!-- web应用初始化参数 -->
  <context-param>
  	<param-name>url</param-name>
  	<param-value>jdbc:mysql://localhost:3306/test</param-value>
  </context-param>
  
   <listener>
   	<description>servletcontext监听器</description>
  	<listener-class>com.test.servletcontextlistener.MyServletContextListener</listener-class>
   </listener>
   <listener>
   	<listener-class>com.test.sessionlistener.MyHttpSessionListener</listener-class>
   </listener>
   <listener>
   	<listener-class>com.test.servletrequestlistener.MyServletRequestListener</listener-class>
   </listener>
   <listener>
   	<listener-class>com.test.servletcontextattributelistener.MyServletContextAttributeListener</listener-class>
   </listener>
   <listener>
   	<listener-class>com.test.selfsessiontimer.SessionScanerListener</listener-class>
   </listener>
   
   <filter>
   	<description>注解处理过滤器</description>
   	<filter-name>AnnotationHandlerFilter</filter-name>
   	<filter-class>com.test.annotationservlet.AnnotationHandlerFilter</filter-class>
   	<init-param>
            <description>配置要扫描包及其子包, 如果有多个包,以逗号分隔</description>
            <param-name>basePackage</param-name>
            <param-value>com.test.web.controller</param-value>
            <!-- <param-value>me.gacl.web.controller</param-value> -->
    </init-param>
   </filter>
   
  <filter>
  	<filter-name>TransActionFilter</filter-name>
  	<filter-class>com.test.threadlocal.TransActionFilter</filter-class>
  </filter>
  
  <filter>
  	<filter-name>CharacterEncodingFilter</filter-name>
  	<filter-class>com.test.filterdemo.CharacterEncodingFilter</filter-class>
  	<init-param>
  		<param-name>charset</param-name>
  		<param-value>UTF-8</param-value>
  	</init-param>
  </filter>
  
  <filter>
  	<filter-name>GzipFilter</filter-name>
  	<filter-class>com.test.gzipfilterdemo.GzipFilter</filter-class>
  </filter>
  
   <filter-mapping>
        <filter-name>AnnotationHandlerFilter</filter-name>
        <!-- 拦截后缀是.do的请求 -->
        <url-pattern>*.do</url-pattern>
    </filter-mapping>
  
  <filter-mapping>
  	<filter-name>TransActionFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>CharacterEncodingFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <filter-mapping>
  	<filter-name>GzipFilter</filter-name>
  	<url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  
  <servlet>
  	<servlet-name>ServletDemo01</servlet-name>
  	<servlet-class>com.test.response.ServletDemo01</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDemo02</servlet-name>
  	<servlet-class>com.test.response.ServletDemo02</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDemo03</servlet-name>
  	<servlet-class>com.test.response.ServletDemo03</servlet-class>
  </servlet>
  <servlet>
