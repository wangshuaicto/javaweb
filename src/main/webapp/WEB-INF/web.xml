  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DoFormServlet</servlet-name>
  	<url-pattern>/DoFormServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AnnotationHandlerServlet</servlet-name>
  	<url-pattern>*.action</url-pattern>
  </servlet-mapping>
  
  
  <!-- session过期时间 -->
  <session-config>
  	<session-timeout>1</session-timeout>
  </session-config>
  
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
