  	<servlet-class>com.test.response.ServletDemo05</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDemo06</servlet-name>
  	<servlet-class>com.test.response.ServletDemo06</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDemo07</servlet-name>
  	<servlet-class>com.test.response.ServletDemo07</servlet-class>
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>ws</param-value>
  	</init-param>
  	<init-param>
  		<param-name>password</param-name>
  		<param-value>123456</param-value>
  	</init-param>
  	<init-param>
  		<param-name>charset</param-name>
  		<param-value>UTF-8</param-value>
  	</init-param>
  </servlet>
  <servlet>
  	<servlet-name>ServletDemo08</servlet-name>
  	<servlet-class>com.test.response.ServletDemo08</servlet-class>
  </servlet>
  <servlet>
